<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>

  <%= f.label :artist_name, :Artist %>
  <%= f.text_field :artist_name %>
  <br>

  <%= f.label :genre_id, :Genre %>
  <%= f.select :genre_id, @genre %>
  <br>

  <input type="text" name="song[note_contents][]" id="song_notes_1"><br>
  <input type="text" name="song[note_contents][]" id="song_notes_2"><br>

  <%= f.submit %>
<% end %>
